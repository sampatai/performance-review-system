{
    "family": "PLACEHOLDER_TASK_FAMILY",
    "executionRoleArn": "arn:aws:iam::734100499895:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "3072",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "containerDefinitions": [
        {
            "name": "officepe-review-webapi",
            "image": "PLACEHOLDER_IMAGE_URI",
            "cpu": 0,
            "essential": true,
            "environment": [
                {
                    "name": "ASPNETCORE_ENVIRONMENT",
                    "value": "PLACEHOLDER_ENVIRONMENT"
                },
                {
                    "name": "ASPNETCORE_URLS",
                    "value": "http://+:8080"
                }
            ],
            "portMappings": [
                {
                    "containerPort": 8080,
                    "protocol": "tcp"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/performance-review-api",
                    "awslogs-region": "ap-southeast-2",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "curl -f http://localhost:8080/health || exit 1"
                ],
                "interval": 30,
                "timeout": 5,
                "retries": 3,
                "startPeriod": 60
            }
        }
    ]
}